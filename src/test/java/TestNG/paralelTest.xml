<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Testing " thread-count="2" parallel="tests">

    <test name="RegressionChrome">
    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"/>
        <classes>
            <class name="TestNG.ParallelTesting"/>
        </classes>
    </test> <!-- Test -->

    <test name="RegressionEdge">
        <parameter name="browser" value="edge"/>
        <parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"/>
        <classes>
            <class name="TestNG.ParallelTesting"/>
        </classes>
    </test> <!-- Test -->

</suite> <!-- Suite -->